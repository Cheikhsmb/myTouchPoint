{
  "expo": {
    "name": "myTouchPoint",
    "slug": "myTouchPoint",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/myTouchPointLogo1.png",
    "scheme": "mytouchpoint",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSContactsUsageDescription": "This app accesses your contacts to let you pick phone numbers."
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": ["READ_CONTACTS"]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/myTouchPointLogo1.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/myTouchPointLogo1.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
